{
    "name": "symfony/symfony-demo",
    "license": "MIT",
    "type": "project",
    "description": "Symfony Demo Application",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "replace": {
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "require": {
        "php": ">=8.4",
        "ext-pdo_sqlite": "*",
        "cocur/slugify": "^4.6",
        "doctrine/dbal": "^4.0",
        "doctrine/doctrine-bundle": "*",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^3.0",
        "easycorp/easyadmin-bundle": "^4.24",
        "knplabs/knp-paginator-bundle": "*",
        "league/commonmark": "^2.1",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "^7",
        "symfony/asset-mapper": "^7",
        "symfony/console": "^7",
        "symfony/dotenv": "^7",
        "symfony/event-dispatcher": "^7.2",
        "symfony/expression-language": "^7",
        "symfony/flex": "^2",
        "symfony/form": "^7",
        "symfony/framework-bundle": "^7",
        "symfony/html-sanitizer": "^7",
        "symfony/http-client": "^7",
        "symfony/intl": "^7",
        "symfony/mailer": "^7.2",
        "symfony/mailtrap-mailer": "^7.2",
        "symfony/messenger": "^7",
        "symfony/monolog-bundle": "^3.7",
        "symfony/notifier": "^7",
        "symfony/polyfill-intl-messageformatter": "^1.12",
        "symfony/runtime": "^7",
        "symfony/security-bundle": "^7",
        "symfony/sendgrid-mailer": "^7.2",
        "symfony/stimulus-bundle": "^2.12",
        "symfony/string": "^7.2",
        "symfony/translation": "^7",
        "symfony/twig-bundle": "^7.2",
        "symfony/ux-icons": "^2.27",
        "symfony/ux-live-component": "^2.6",
        "symfony/ux-translator": "^2.24",
        "symfony/validator": "^7",
        "symfony/webpack-encore-bundle": "^2.2",
        "symfony/yaml": "^7",
        "symfonycasts/reset-password-bundle": "^1.23",
        "symfonycasts/sass-bundle": "^0.7",
        "twig/cssinliner-extra": "^3.20",
        "twig/extra-bundle": "^3.20",
        "twig/intl-extra": "^3.3",
        "twig/markdown-extra": "*",
        "twig/string-extra": "^3.20",
        "vich/uploader-bundle": "^2.5"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^8.0.2",
        "doctrine/doctrine-fixtures-bundle": "^3.5",
        "phpstan/phpstan": "^2.0",
        "phpstan/phpstan-doctrine": "^2.0",
        "phpstan/phpstan-symfony": "^2.0",
        "phpunit/phpunit": "^12.0",
        "symfony/browser-kit": "^7",
        "symfony/css-selector": "^7",
        "symfony/debug-bundle": "^7",
        "symfony/maker-bundle": "^1.62",
        "symfony/phpunit-bridge": "^7",
        "symfony/stopwatch": "^7",
        "symfony/web-profiler-bundle": "^7",
        "twbs/bootstrap": "^5"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "platform": {
            "php": "8.4.3"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd",
            "sass:build": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "^7.2"
        }
    }
}
